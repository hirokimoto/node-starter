{
  "name": "node-app",
  "version": "1.0.0",
  "description": "Node.JS Application with MySQL, Sequelize, Express",
  "main": "dist/app.js",
  "scripts": {
    "start-prod": "NODE_ENV=production node ./dist/server.js",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./server.ts",
    "tslint": "tslint --project tsconfig.json",
    "tslint:fix": "tslint --project tsconfig.json --fix",
    "start-dev": "nodemon",
    "test": "mocha -r ts-node/register test/**/*.test.ts",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.test.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirokimoto/node-starter.git"
  },
  "keywords": [],
  "author": "Hiroki Moto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hirokimoto/node-starter/issues"
  },
  "homepage": "https://github.com/hirokimoto/node-starter#readme",
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "src/"
    ],
    "ignore": [
      "dist/*",
      "node_modules/*"
    ],
    "ext": "ts",
    "exec": "npm run dev"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.3",
    "@types/cors": "^2.8.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.18",
    "@types/node": "^20.7.2",
    "@types/sequelize": "^4.28.16",
    "@types/supertest": "^2.0.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.1.0",
    "aws-sdk": "^2.1467.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "googleapis": "^126.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.6.1",
    "nodemailer": "^6.9.5",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-oidc": "^0.1.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "readline": "^1.3.0",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "uuid": "^9.0.1"
  }
}
